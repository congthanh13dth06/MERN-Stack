{
  "name": "WEB-FULLSTACK",
  "version": "0.0.1",
  "description": "",
  "main": "server/src/app.server.js",
  "private": true,
  "scripts": {
    "dev:clear": "export mode=none || set mode=production && rm -rf ./server/src/build",
    "dev:client": "cd ./client && npm run build && mv ./build ../server/src && cd ../",
    "dev:server": "nodemon --exec babel-node ./server/src/bin/www",
    "dev": "npm-run-all -p dev:clear dev:client && npm run dev:server",
    "build:client": "cp -r ./server/src/build ./dist",
    "build:server": "webpack --config webpack.config.js",
    "build:clear": "export mode=none || set mode=none && rm -rf ./dist",
    "build": "npm-run-all -p build:clear build:server && npm run build:client",
    "dboff": "killall mongod",
    "dbon": "mkdir -p ./db && mongod --dbpath ./db"
  },
  "author": "Thanh Nguyen",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "body-parser": "^1.18.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "helmet": "^3.21.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "mongoose": "^5.7.8",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  }
}
